/**app.wxss**/
.container {
  display: flex;
  box-sizing: border-box;
  color: #6B6B6B;
} 

.tapContainer {
  width: 100%;
  padding: 0;
  border-radius: 0px;
  background: #F3F4F8;
  color: #6B6B6B;
}

.tapContainer::after{
  border: none;
}

.right_icon_url {
  width: 20px;
  height: 20px;
}

.tapLogin {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: transparent;
}

.tapLogin::after{
  border: none;
}