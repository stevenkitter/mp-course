/**index.wxss**/
.container {
  background: green;
  flex-direction: column;
  color: #6B6B6B;
}

.tapContainer {
  width: 100%;
  padding: 0;
  border-radius: 0px;
  background: #F3F4F8;
}

.tapContainer::after{
  border: none;
}

.swiper {
  width: 100%;
}

.modules {
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding: 0 15px;
  color: #6B6B6B;
}

.cources {
  margin-top: 10px;
  background: #fff;
}

.cources-title {
  display: flex;
  margin-left: 12px;
  margin-top: 12px;
  color: #6B6B6B;
  font-weight: 500;
}